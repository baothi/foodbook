<!DOCTYPE html>
<html>
<head>
  <title>Foodbook</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>

<%= render "layouts/header" %>

<div class="container">
  <div class="row">
    <%= render "layouts/aside" %>
    <div class="col-md-9">
      <%= yield %>
    </div>
  </div>
</div>
</body>
</html>
